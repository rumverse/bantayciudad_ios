


inhibit_all_warnings!

target "BantayCiudad" do

source 'https://github.com/CocoaPods/Specs.git'

# RestKit - RestKit is a modern Objective-C framework for implementing RESTful web services clients on iOS and Mac OS X.
# Github Link: https://github.com/RestKit/RestKit
pod 'RestKit', '~> 0.23.1'

# MagicalRecord - Super Awesome Easy Fetching for Core Data.
# Github Link: https://github.com/magicalpanda/MagicalRecord
pod 'MagicalRecord', :git => 'https://github.com/magicalpanda/MagicalRecord.git', :tag => 'v2.3.0-beta.3'

pod 'SDWebImage', '~> 3.7'

pod 'Google-Maps-iOS-SDK', '~> 1.9'

pod 'UIColor+BFPaperColors', '~>1.3'

end